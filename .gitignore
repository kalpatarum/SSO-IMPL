# ========================
# Java Specific Exclusions
# ========================

# Compiled class files
*.class

# Package Files
*.jar
*.war
*.ear
*.nar
*.zip
*.tar.gz
*.rar

# Log files
*.log
logs/

# Build artifacts (Maven, Gradle, etc.)
# Maven
target/
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties
dependency-reduced-pom.xml
buildNumber.properties
.mvn/timing.properties
.mvn/wrapper/maven-wrapper.jar

# Gradle
.gradle/
/build/
.gradletasknamecache
gradle-app.setting
!gradle-wrapper.jar # Don't ignore the Gradle wrapper JAR
gradle-wrapper.properties

# IntelliJ IDEA (common for Java development)
.idea/
.idea_modules/
out/
*.iml
*.ipr
*.iws

# Eclipse (another common Java IDE)
.project
.classpath
.settings/
bin/
tmp/
.metadata
hs_err_pid* # JVM crash logs

# NetBeans
nbproject/private/
build/
nbbuild/
dist/
.nb-gradle/
.nb_antlr_generated/

# Checkpoint files
*.orig

# Mobile Tools for Java (J2ME)
.mtj.tmp/

# ========================
# Node.js Specific Exclusions
# ========================

# Dependency directories
node_modules/
/jspm_packages/ # JSPM dependency directory
/web_modules/   # Snowpack dependency directory

# Local .npm, .yarn, .pnpm caches
.npm/
.yarn/
.pnpm-store/

# Optional ESLint cache
.eslintcache

# Optional Stylelint cache
.stylelintcache

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# Diagnostic reports (Node.js versions 8 and later)
report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json

# Runtime data
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
/coverage
.nyc_output/ # nyc test coverage

# Build outputs (common for frontend frameworks)
/build
/dist
/out
.next/ # Next.js build output
.nuxt/ # Nuxt.js build output
.cache/ # Gatsby, Parcel, other bundler caches
.parcel-cache/ # Parcel-bundler cache
.vuepress/dist # Vuepress build output
.docusaurus/ # Docusaurus cache and generated files

# Environment variables
.env
.env.*.local # e.g., .env.development.local, .env.test.local, .env.production.local
.env.local

# TypeScript cache
*.tsbuildinfo

# Microbundle cache
.rpt2_cache/
.rts2_cache_cjs/
.rts2_cache_es/
.rts2_cache_umd/

# Serverless directories
.serverless/

# FuseBox cache
.fusebox/

# DynamoDB Local files
.dynamodb/

# TernJS port file
.tern-port

# Stores VSCode versions used for testing VSCode extensions
.vscode-test/

# Yarn v2
.yarn/cache
.yarn/unplugged
.yarn/build-state.yml
.yarn/install-state.gz
.pnp.*

# Visual Studio Code (common for many languages)
.vscode/

# MacOS specific files
.DS_Store

# Thumbnails
.localized